	var degrees =3D =
['init_degree','first_degree','second_degree','third_degree'];
	var getIndexOfDegree =3D function(clazz){
		var clazzs =3D clazz.split(' ');
		for(var c in clazzs){
			var cur =3D clazzs[c];
			for (var i =3D degrees.length - 1; i >=3D 0; i--) {
				if(degrees[i] =3D=3D cur){
